<div class="row">
    <div class="span2">
<h2>Leads</h2>
</div>
<div class="span3 offset7">
<%= link_to 'Add New Lead', new_lead_path, class: 'btn btn-primary' %>
</div>
</div>
<table class="table table-striped">
  <tr>
    <th>Company</th>
    <th>Contact</th>
    <th>Phone</th>
    <th>Website</th>
    <th>Email</th>
    <th>Notes</th>
    <th>Last Updated</th>    
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @leads.each do |lead| %>
  <tr>
    <td><%= lead.company_name %></td>
    <td><%= lead.contact %></td>
    <td><%= lead.phone %></td>
    <td><%= lead.website %></td>
    <td><%= lead.email %></td>
    <td><%= lead.notes %></td>
    <td><%= lead.updated_at %></td>    
    <td><%= link_to 'Show', lead %></td>
    <td><%= link_to 'Edit', edit_lead_path(lead) %></td>
    <td><%= link_to 'Destroy', lead, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>
<br />


