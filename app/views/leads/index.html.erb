<h1>Listing leads</h1>

<table>
  <tr>
    <th>Company name</th>
    <th>Contact</th>
    <th>Phone</th>
    <th>Website</th>
    <th>Notes</th>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @leads.each do |lead| %>
  <tr>
    <td><%= lead.company_name %></td>
    <td><%= lead.contact %></td>
    <td><%= lead.phone %></td>
    <td><%= lead.website %></td>
    <td><%= lead.notes %></td>
    <td><%= lead.email %></td>
    <td><%= link_to 'Show', lead %></td>
    <td><%= link_to 'Edit', edit_lead_path(lead) %></td>
    <td><%= link_to 'Destroy', lead, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Lead', new_lead_path %>
